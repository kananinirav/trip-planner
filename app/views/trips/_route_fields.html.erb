<div class="nested-fields mt-2">
  <div class="row">
    <div class="col-md-3">
      <%= f.label :place_name %>
      <%= f.text_field :place_name, class: "form-control" %>
    </div>
    <div class="col-md-2">
      <%= f.label :start_date %>
      <%= f.date_field :start_date, class: "form-control" %>
    </div>
    <div class="col-md-2">
      <%= f.label :end_date%>
      <%= f.date_field :end_date, class: "form-control" %>
    </div>
    <div class="col-md-2">
      <%= link_to_remove_association "Delete", f, :class => "btn btn-danger  mt-4" %>
    </div>
  </div>
</div>
